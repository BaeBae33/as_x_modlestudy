<manifest
    package="com.data.network.aidl.provider"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name="com.aidl.provider.MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.aidl.provider.activity.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <!-- 提供服务的service -->
        <service android:name="com.aidl.provider.server.ProviderService">
            <intent-filter>
                <action android:name="com.yline.aidl.provider"/>
            </intent-filter>
        </service>
    </application>

</manifest>
