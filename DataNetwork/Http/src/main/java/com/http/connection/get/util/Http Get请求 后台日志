【请求头:9条】
// 增加
header,key = accept,value = */*
header,key = connection,value = Keep-Alive
header,key = content-type,value = application/x-www-form-urlencoded
header,key = charset,value = utf-8
header,key = action,value = get
// 自带
header,key = user-agent,value = Dalvik/1.6.0 (Linux; U; Android 4.4.2; HUAWEI P7-L05 Build/HuaweiP7-L05)
header,key = host,value = 192.168.1.102:8080
header,key = accept-encoding,value = gzip
header,key = content-length,value = 0 // 请求体的长度,请求头的;
【请求参数,长连接中的拼接部分】
parameter,key = json,value = "json"
