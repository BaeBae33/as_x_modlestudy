<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/btn_general_append"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="parse general append"/>

    <Button
        android:id="@+id/btn_general_serializer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="parse general serializer"/>

    <Button
        android:id="@+id/btn_parse_pull"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="parse pull"/>

    <Button
        android:id="@+id/btn_parse_sax"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="parse sax"/>

</LinearLayout>