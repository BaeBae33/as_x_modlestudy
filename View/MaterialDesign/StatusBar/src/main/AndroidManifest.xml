<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.status">

    <application
        android:name="com.yline.application.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppBaseTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.ColorStatusBarActivity">
        </activity>
        <activity android:name=".activity.ImageStatusBarActivity">
        </activity>
        <activity android:name=".activity.ImageViewActivity">
        </activity>
        <activity android:name=".activity.UseInFragmentActivity">
        </activity>
        <activity android:name=".activity.SwipeBackActivity">
        </activity>
    </application>

</manifest>