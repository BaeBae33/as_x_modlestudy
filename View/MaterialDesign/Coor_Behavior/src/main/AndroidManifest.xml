<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yline.coor.behavior">

    <application
        android:name=".MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".type1.Type1Activity" />
        <activity android:name=".type2.Type2Activity" />
        <activity
            android:name=".type3.Type3Activity"
            android:theme="@style/NoBarTheme" />
        <activity android:name=".type4.Type4Activity" />
    </application>

</manifest>