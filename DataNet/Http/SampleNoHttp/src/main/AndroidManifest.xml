<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.nohttp">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:name=".activity.IApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.Default.NoActionBar">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.OriginalActivity"/>
        <activity android:name=".activity.MethodActivity"/>
        <activity android:name=".activity.ImageActivity"/>
        <activity android:name=".activity.JsonActivity"/>
        <activity android:name=".activity.PostBodyActivity"/>
        <activity android:name=".activity.DefineRequestActivity"/>
        <activity android:name=".activity.CacheActivity"/>
        <activity android:name=".activity.RedirectActivity"/>
        <activity android:name=".activity.CacheHttpActivity"/>
        <activity android:name=".activity.CacheRequestFailedReadCacheActivity"/>
        <activity android:name=".activity.CacheNoneCacheRequestNetWorkActivity"/>
        <activity android:name=".activity.CacheOnlyReadCacheActivity"/>
        <activity android:name=".activity.CacheOnlyRequestNetworkActivity"/>
    </application>

</manifest>